<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This Custom Setting maps the Country against SalesOrg and Company Code (Seed Data processor.)</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Company_Code__c</fullName>
        <description>This derives the company code for the Seed data, to get the pricing and availability based on the Country and sales org defined.</description>
        <externalId>false</externalId>
        <label>Company Code</label>
        <length>5</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Country_del__c</fullName>
        <description>Country for Seed Data Pricing and availability.</description>
        <externalId>false</externalId>
        <label>Country</label>
        <length>20</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Sales_Org__c</fullName>
        <description>This derives the sales org for the Seed Data based on the country entered.</description>
        <externalId>false</externalId>
        <label>Sales Org</label>
        <length>5</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Country-SalesOrg-CompanyCode</label>
    <visibility>Public</visibility>
</CustomObject>
